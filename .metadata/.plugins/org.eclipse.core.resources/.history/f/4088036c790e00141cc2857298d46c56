package com.gling.bookmeup;

import android.app.Application;
import android.util.Log;

import com.parse.Parse;
import com.parse.ParseAnalytics;
import com.parse.ParseObject;
import com.parse.ParseInstallation;
import com.parse.PushService;

public class BookMeUpApplication extends Application {

	private final String TAG = "BookMeUpApplication";
	private final String PARSE_APPLICATION_ID = "0Uye8FHMnsklraYbqnMDxtg0rbQRKEqZSVO6BHPa";
	private final String PARSE_CLIENT_KEY = "5dB8I0UZWFaTtYpE3OUn7CWwPzxYxe2yBqE7uhS3";

	@Override
	public void onCreate() {
		Log.i(LOG, "onCreate");
		super.onCreate();

		// Initialize parse
		Parse.initialize(this, PARSE_APPLICATION_ID, PARSE_CLIENT_KEY);

		// Configure parse push service
		PushService.setDefaultPushCallback(this, MainActivity.class);
		ParseInstallation.getCurrentInstallation().saveInBackground();
	}
}
